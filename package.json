{
  "name": "dependabot-automerge-demo-app",
  "version": "1.0.0",
  "description": "Sample application demonstrating Dependabot auto-merge pipeline",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:e2e": "jest --testPathPattern=test/e2e",
    "test:smoke": "jest --testPathPattern=test/smoke",
    "test:smoke:quick": "jest --testPathPattern=test/smoke --maxWorkers=2",
    "test:performance": "jest --testPathPattern=test/performance",
    "test:regression": "jest --testPathPattern=test/regression",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "format": "prettier --write 'src/**/*.js' 'test/**/*.js'",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production --env production",
    "security-check": "npm audit",
    "dependency-check": "npm-check-updates",
    "clean": "rm -rf dist coverage node_modules",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "dotenv": "^16.0.3",
    "winston": "^3.8.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "body-parser": "^1.20.2",
    "uuid": "^9.0.0",
    "jsonwebtoken": "^9.0.0",
    "bcrypt": "^5.1.0",
    "mongoose": "^7.0.3",
    "redis": "^4.6.5",
    "pg": "^8.10.0",
    "mysql2": "^3.2.0",
    "sequelize": "^6.31.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-security": "^1.7.1",
    "prettier": "^2.8.7",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "nodemon": "^2.0.22",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "babel-loader": "^9.1.2",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "npm-check-updates": "^16.10.8"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinite-kube/dependabot-automerge-pipeline-demo.git"
  },
  "keywords": [
    "dependabot",
    "automation",
    "ci-cd",
    "jenkins",
    "auto-merge",
    "dependency-management"
  ],
  "author": "InfiniteKube Team",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  }
}
