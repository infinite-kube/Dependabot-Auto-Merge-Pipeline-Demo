{
  "name": "dependabot-automerge-demo-app",
  "version": "1.0.0",
  "description": "Sample application demonstrating Dependabot auto-merge pipeline",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:e2e": "jest --testPathPattern=test/e2e",
    "test:smoke": "jest --testPathPattern=test/smoke",
    "test:smoke:quick": "jest --testPathPattern=test/smoke --maxWorkers=2",
    "test:performance": "jest --testPathPattern=test/performance",
    "test:regression": "jest --testPathPattern=test/regression",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "format": "prettier --write 'src/**/*.js' 'test/**/*.js'",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production --env production",
    "security-check": "npm audit",
    "dependency-check": "npm-check-updates",
    "clean": "rm -rf dist coverage node_modules",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "express": "4.17.1",
    "axios": "0.21.1",
    "lodash": "4.17.19",
    "moment": "2.29.1",
    "dotenv": "8.2.0",
    "winston": "3.3.3",
    "cors": "2.8.5",
    "helmet": "4.6.0",
    "compression": "1.7.4",
    "body-parser": "1.19.0",
    "uuid": "8.3.2",
    "jsonwebtoken": "8.5.1",
    "bcrypt": "5.0.0",
    "mongoose": "5.12.3",
    "redis": "3.1.2",
    "pg": "8.5.1",
    "mysql2": "2.2.5",
    "sequelize": "6.6.2",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "minimist": "1.2.5",
    "node-fetch": "2.6.1",
    "glob-parent": "5.1.1",
    "tar": "6.1.0",
    "path-parse": "1.0.6",
    "trim-newlines": "3.0.0",
    "hosted-git-info": "2.8.8",
    "y18n": "4.0.0",
    "underscore": "1.12.0",
    "ini": "1.3.5",
    "bl": "4.0.3",
    "node-notifier": "8.0.1",
    "yargs-parser": "18.1.3"
  },
  "devDependencies": {
    "@types/node": "14.14.37",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "eslint": "7.23.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-security": "1.4.0",
    "prettier": "2.2.1",
    "jest": "26.6.3",
    "supertest": "6.1.3",
    "nodemon": "2.0.7",
    "webpack": "5.103.0",
    "webpack-cli": "4.6.0",
    "babel-loader": "8.2.2",
    "@babel/core": "7.13.14",
    "@babel/preset-env": "7.13.12",
    "husky": "5.2.0",
    "lint-staged": "10.5.4",
    "npm-check-updates": "11.3.0",
    "handlebars": "4.7.6",
    "highlight.js": "10.4.1",
    "marked": "2.0.0",
    "serialize-javascript": "5.0.0",
    "elliptic": "6.5.3",
    "ssri": "8.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinite-kube/dependabot-automerge-pipeline-demo.git"
  },
  "keywords": [
    "dependabot",
    "automation",
    "ci-cd",
    "jenkins",
    "auto-merge",
    "dependency-management"
  ],
  "author": "InfiniteKube Team",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  }
}
